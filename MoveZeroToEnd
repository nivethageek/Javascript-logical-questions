function pushZerosToEnd(arr) {
  let count = 0;
  for (let i = 0; i < arr.length; i++) {
    if (arr[i] !== 0) {
      arr[count++] = arr[i];
    }
  }
  while (count < arr.length) {
    arr[count++] = 0;
  }
}
const arr = [1, 9, 8, 4, 0, 0, 2, 7, 0, 6, 0, 9];
pushZerosToEnd(arr);
console.log(arr); // o/p : [1, 9, 8, 4, 2, 7, 6, 9, 0, 0, 0, 0]
